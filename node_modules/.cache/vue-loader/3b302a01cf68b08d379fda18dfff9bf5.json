older=\"请输入用户名\"></el-input>\n          </el-form-item>\n          <el-form-item prop=\"email\" label=\"邮箱\">\n            <el-input prefix-icon=\"iconfont iconyouxiang\" v-model=\"user.email\" placeholder=\"请输入邮箱\"></el-input>\n          </el-form-item>\n          <el-form-item prop=\"password\" label=\"密码\">\n            <el-input prefix-icon=\"iconfont iconmimacopy\" v-model=\"user.password\" show-password placeholder=\"请输入密码\"></el-input>\n          </el-form-item>\n          <el-form-item prop=\"password\" label=\"确认密码\">\n            <el-input prefix-icon=\"iconfont iconmimacopy\" v-model=\"user.password\" show-password placeholder=\"确认密码\"></el-input>\n          </el-form-item>\n          <el-form-item>\n            <el-button type=\"primary\" icon @click=\"doRegister()\">注册</el-button>\n          </el-form-item>\n        </el-form>\n      </el-row>\n    </div>\n  </div>\n</template>\n \n<script>\nimport axios from \"axios\";\nimport qs from 'qs';\nexport default {\n  name: \"login\",\n  data() {\n    return {\n      user: {\n        tel: \"123456789\",\n        username: \"abcd\",\n        email: \"abcd@test.com\",\n        password: \"12345\"\n      },\n    };\n  },\n  created() {//来发起当前组件的数据请求\n    // console.log($);\n    // console.log(\"1111\");\n  },\n  methods: {\n    doRegister() {\n      if (!this.user.username) {\n        this.$message.error(\"请输入用户名！\");\n        return;\n      } else if (!this.user.email) {\n        this.$message.error(\"请输入邮箱！\");\n        return;\n      } else if (this.user.email != null) {\n        var reg = /^[A-Za-z0-9\\u4e00-\\u9fa5]+@[a-zA-Z0-9_-]+(\\.[a-zA-Z0-9_-]+)+$/;\n        if (!reg.test(this.user.email)) {\n          this.$message.error(\"请输入有效的邮箱！\");\n        } else if (!this.user.password) {\n          this.$message.error(\"请输入密码！\");\n          return;\n        } else {\n          //this.$router.push({ path: \"/\" }); //无需向后台提交数据，方便前台调试\n          this.$refs.loginForm.validate((valid) => {\n        if (!valid) return;\n       const result = this.$http.post('/user/new', qs.stringify(this.user)).then((res)=>{ \n         if (res.data.errorCode == 0) \n         {\n          this.$message({\n            showClose: true,\n            message: '注册成功!',\n            type: 'success'\n          });\n          this.$router.push('/')\n         };\n       })\n      });\n        }\n      }\n    }\n  }\n};\n</script>\n \n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.login {\n  width: 100%;\n  height: 840px;\n  background: url(\"../assets/images/bg1.png\") no-repeat;\n  background-size: cover;\n  overflow: hidden;\n  margin-bottom: px;\n}\n.login-wrap {\n  background: url(\"../assets/images/login_bg2.png\") no-repeat;\n  background-size: cover;\n  width: 400px;\n  height: 500px;\n  margin: 210px auto;\n  overflow: hidden;\n  padding-top: 3px;\n  line-height: 0px;\n}\n \nh3 {\n  color: #030303b8;\n  font-size: 24px;\n  margin-top: 50px;\n}\nform {\n    display: block;\n    margin-top: 35px;\n}\n.el-button--primary {\n    color: #FFF;\n    background-color: #3962d6;\n    border-color: #3962d6;\n    text-align:center;\n}\n.el-button {\n  width: 170%;\n  margin-left: -105px;\n}\n</style>\n"]}]}